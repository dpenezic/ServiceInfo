<!-- Page Head -->
<div class="page-head">
    <div class="container">
        <div class="row">
            {{#if feedback }}
                <div class="page-title" data-i18n="Feedback-Search.Page-Head.Title"></div>
                <div class="page-descr" data-i18n="Feedback-Search.Page-Head.Description"></div>
            {{else}}
                <div class="page-title" data-i18n="Search-List.Page-Head.Title"></div>
                <div class="page-descr" data-i18n="Search-List.Page-Head.Description"></div>
            {{/if}}
        </div>
    </div>
</div>
<!-- End Page Head -->

<div id="search" class="search-list">
    <!-- "floating" button -->
    <div class="float-bottom-end">
        <a href="#/service/request">
            <button class="primary-button" data-i18n="Search-List.Service-Request" id="request_service_button"></button>
        </a>
    </div>

    <!-- Main Content -->
    <div class="container">
        <div id="search_controls"></div>

        <div class="row no-search-results">
          <span data-i18n="Service-Map.No-Search-Results"></span>
        </div>

        <div class="row results-truncated">
            <span data-i18n="Service-Map.Results-Truncated"></span>
        </div>

        <div class="row">
            <ul class="search-result-list"></ul>
        </div> <!-- End Row -->
    </div> <!-- End Main Content -->
</div>
