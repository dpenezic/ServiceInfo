#: email_user/templates/registration/password_reset_email.html:2
#, python-format
msgid ""
"You're receiving this email because you requested a password reset for your "
"user account at %(site_name)s."
msgstr "Vous recevez cet email car vous avez demandé une réinitialisation de mot de passe pour votre compte utilisateur au %(site_name)s."

#: email_user/templates/registration/password_reset_email.html:4
msgid "Please go to the following page and choose a new password:"
msgstr "S'il vous plaît aller à la page suivante et choisir un nouveau mot de passe:"

#: email_user/templates/registration/password_reset_email.html:8
msgid "Your username is your email address:"
msgstr "Votre nom d'utilisateur est votre adresse email:"

#: email_user/templates/registration/password_reset_email.html:10
